{
  "name": "vite-react-ts",
  "version": "0.0.3",
  "description": "a Vite2 + Typescript + React + Antd + Less + Eslint + Prettier template",
  "homepage": "https://github.com/JS-banana/vite-react-ts",
  "repository": {
    "type": "git",
    "url": "git@github.com:JS-banana/vite-react-ts.git"
  },
  "author": {
    "name": "JS-banana",
    "email": "sss213018@163.com",
    "url": "https://ssscode.com",
    "github": "https://github.com/JS-banana"
  },
  "keywords": [
    "vite",
    "typescript",
    "react",
    "zustand",
    "antd",
    "less",
    "eslint",
    "prettier"
  ],
  "license": "MIT",
  "scripts": {
    "postinstall": "husky install",
    "serve": "npm run dev",
    "dev": "vite --mode development",
    "build": "vite build",
    "build:beta": "vite build --mode beta",
    "build:release": "vite build --mode release",
    "build:legacy": "vite build --mode legacy ",
    "build:deploy": "vite build --base=/vite-react-ts/",
    "preview": "vite preview",
    "prebuild": "rimraf dist",
    "deploy": "gh-pages -d dist",
    "commitlint": "commitlint --from=master",
    "lint:fix": "eslint --cache --ext .js,.jsx,.ts,.tsx --no-error-on-unmatched-pattern --quiet --fix ./src",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "clean:cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
    "stylelint-check": "stylelint-config-prettier-check",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "lint-staged": {
    "**/*.{js,jsx,tsx,ts,json}": [
      "npm run lint:fix",
      "git add --force"
    ],
    "**/*.{less}": [
      "npm run lint:style",
      "git add --force"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/icons": "^4.6.2",
    "@ant-design/pro-layout": "^6.19.3",
    "antd": "^4.14.1",
    "axios": "^0.21.1",
    "dayjs": "^1.10.6",
    "mockjs": "^1.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.2.0",
    "zustand": "^3.5.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@types/babel__core": "^7.1.14",
    "@types/node": "^14.14.36",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/react-router-config": "^5.0.3",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "@vitejs/plugin-legacy": "^1.4.4",
    "@vitejs/plugin-react-refresh": "^1.3.5",
    "autoprefixer": "^10.3.1",
    "conventional-changelog-cli": "^2.1.1",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "gh-pages": "^3.2.3",
    "husky": "^7.0.1",
    "less": "^4.1.1",
    "less-vars-to-js": "^1.3.0",
    "lint-staged": "^11.1.2",
    "postcss": "^8.3.6",
    "pre-commit": "^1.2.2",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "rollup-plugin-visualizer": "^5.5.2",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recess-order": "^2.4.0",
    "stylelint-config-standard": "^22.0.0",
    "typescript": "^4.2.3",
    "vite": "^2.1.3",
    "vite-aliases": "^0.5.1",
    "vite-plugin-babel-import": "^2.0.2",
    "vite-plugin-compression": "^0.3.1",
    "vite-plugin-imp": "^2.0.7",
    "vite-plugin-mock": "^2.9.4",
    "vite-plugin-style-import": "^1.0.1"
  }
}
